{
  "name": "leadsengage/api-library",
  "license": "MIT",
  "type": "library",
  "description": "Leadsengage API Connector Library",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Leadsengage\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Leadsengage\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.3.7",
    "ext-curl": "*",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "raveren/kint": "~0.9",
    "phpunit/phpunit": "~4.8|~5.0"
  }
}
